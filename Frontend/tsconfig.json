{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "paths": {
            "@/*": ["./*"]
        },
        "jsx": "react-jsx"
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts",
        "app/home/index.tsx",
        "screens/Album.tsx",
        "screens/Film.tsx",
        "components/Show.ksx",
        "app/(drawer)/(finished_content_tabs)/finishedContentAlbum.jsx",
        "components/TopScreenFunctionality.tsx",
        "components/ContentCard.tsx"
    ]
}
